<h1>New Organizer</h1>

<table class="table table-hover">
<thead>
  <tr>
    <th scope="col">Name</th>
    <th></th>
    </thead>
  </tr>
 
 <tbody>
  <% @organizers.each do |organizer| %>
    <tr class="table-active">
       <td><%= organizer.name %></td>
      <td><%= link_to 'Show', organizer_path(organizer) %></td>
      <td><%= link_to  'Edit', edit_organizer_path(organizer) %>
      <td><%= link_to  'Destroy', organizer_path(organizer), method: :delete,
      data: { confirm: "Are you sure?"} %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%=link_to 'Back', organizers_path %>
